    "acorn": "^8.11.2",
    "estree-util-to-js": "^2.0.0",
    "estree-walker": "^3.0.3",
    "gray-matter": "^4.0.3",
    "hast-util-from-html": "^2.0.3",
    "hast-util-to-html": "^9.0.4",
    "hast-util-to-text": "^4.0.2",
