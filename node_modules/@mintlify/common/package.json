    "@types/hast": "^3.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/mdast": "^4.0.4",
    "@typescript-eslint/parser": "6.x",
    "eslint": "8.x",
    "openapi-types": "^12.0.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.1",
    "typescript": "^5.5.3",
